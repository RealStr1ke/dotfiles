# npm (https://docs.npmjs.com/cli/completion)

if is-executable npm; then
  . <(npm completion)
fi

# fnm

if is-executable fnm; then
  . <(fnm completions)
fi

# Git

if is-executable git; then
	curl -fsSL "https://raw.githubusercontent.com/git/git/master/contrib/completion/git-completion.bash" | bash
fi
